  .data
  .globl var
var:
  .zero 4

  .text
  .globl f
f:
  addi sp, sp, -16
  la t0, var
  lw t0, 0(t0)
  sw t0, 0(sp)
  lw t0, 0(sp)
  li t1, 1
  add t0, t0, t1
  sw t0, 4(sp)
  lw t0, 4(sp)
  la t1, var
  sw t0, 0(t1)
  li a0, 2
  addi sp, sp, 16
  ret

  .text
  .globl main
main:
  addi sp, sp, -16
  sw ra, 12(sp)
  call f
  sw a0, 0(sp)
  la t0, var
  lw t0, 0(t0)
  sw t0, 4(sp)
  lw a0, 4(sp)
  lw ra, 12(sp)
  addi sp, sp, 16
  ret