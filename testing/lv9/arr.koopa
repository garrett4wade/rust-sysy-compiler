global @ga = alloc [i32, 10], zeroinit
global @gb = alloc [[i32, 3], 2], {{5, 6, 7}, {8, 0, 0}}

decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @main(): i32 {
%entry:
  @a = alloc [i32, 10]
  @b = alloc [[i32, 3], 2]
  %0 = getelemptr @b, 0
  %1 = getelemptr %0, 0
  store 1, %1
  %2 = getelemptr @b, 0
  %3 = getelemptr %2, 1
  store 2, %3
  %4 = getelemptr @b, 0
  %5 = getelemptr %4, 2
  store 3, %5
  %6 = getelemptr @b, 1
  %7 = getelemptr %6, 0
  store 4, %7
  %8 = getelemptr @b, 1
  %9 = getelemptr %8, 1
  store 0, %9
  %10 = getelemptr @b, 1
  %11 = getelemptr %10, 2
  store 0, %11
  @sum = alloc i32
  store 0, @sum
  %12 = getelemptr @gb, 1
  %13 = getelemptr %12, 2
  %14 = load %13
  ret %14
}
